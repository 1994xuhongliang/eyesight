.result {
  width: 750rpx;
  height: 1206rpx;
  background: #cae9fe;
  box-sizing: border-box;
  padding: 60rpx 0 0;
}
.result .bottom-img {
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 750rpx;
  height: 151rpx;
  background: url(../../public/img/result-1.png) 0 0 no-repeat;
  background-size: 100% auto;
}
.result .card {
  position: relative;
  z-index: 100;
  width: 522rpx;
  height: 747rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 10rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100% auto;
}

.result .result-icon {
  position: absolute;
  width: 129rpx;
  height: 124rpx;
  right: -38rpx;
  top: 45rpx;
}
.result .header {
  text-align: center;
  font-size: 30rpx;
  color: #e0ffff;
  line-height: 65rpx;
  margin: 0 auto 80rpx;
}

.result .result-text-1 {
  display: flex;
  align-items: baseline;
  justify-content: flex-start;
  font-size: 30rpx;
  color: rgba(55, 55, 55, 1);
  box-sizing: border-box;
  padding: 0 80rpx;
  margin: 0 auto 20rpx;
}
.result .result-text-2 {
  height: 100rpx;
  display: flex;
  align-items: baseline;
  justify-content: flex-start;
  font-size: 30rpx;
  line-height: 1.5;
  color: rgba(55, 55, 55, 1);
  box-sizing: border-box;
  padding: 0 80rpx;
}

.result .result-text-1 .value {
  font-size: 40rpx;
  color: rgba(208, 63, 33, 1);
}


.result .test-items {
  height: 380rpx;
  margin: 0 auto 0;
  box-sizing: border-box;
  padding: 60rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.result .test-items .test-item {
  /* margin: 0 auto 40rpx; */
  width: 100%;
  box-sizing: border-box;
  padding: 0 80rpx;
  display: flex;
  align-items: baseline;
  justify-content: space-between;
  font-size: 30rpx;
 color: rgba(55, 55, 55, 1);
}

.result .test-items .test-item .value {
  color: rgba(208, 63, 33, 1);
}

.result .test-items .test-item.has-value .value {
   color: rgba(110, 189, 87, 1);
}
.share {
  width: 520rpx;
  color: white;
  height: 80rpx;
  font-size: 30rpx;
  background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
  border-radius: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx auto 0;
}

.button-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 60rpx;
  margin: 100rpx tuo 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.button-box .each-button {
  width: 290rpx;
  color: white;
  height: 65rpx;
  font-size: 30rpx;
  background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
  border-radius: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx auto 0;
}