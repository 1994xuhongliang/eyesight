.result {
  width: 750rpx;
  height: 1206rpx;
  background: #cae9fe;
  box-sizing: border-box;
  padding: 60rpx 0 0;
}
.result .bottom-img {
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 750rpx;
  height: 151rpx;
  background: url(../../public/img/result-1.png) 0 0 no-repeat;
  background-size: 100% auto;
}
.result .card {
  position: relative;
  z-index: 100;
  width: 522rpx;
  height: 747rpx;
  background: #fff;
  margin: 0 auto;
  border-radius: 10rpx;
  box-sizing: border-box;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100% auto;
}

.result .result-icon {
  position: absolute;
  width: 129rpx;
  height: 124rpx;
  right: -38rpx;
  top: 45rpx;
}
.result .header {
  text-align: center;
  font-size: 30rpx;
  color: #e0ffff;
  line-height: 65rpx;
  margin: 0 auto 80rpx;
}

.result .result-text-1 {
  display: flex;
  align-items: baseline;
  justify-content: flex-start;
  font-size: 30rpx;
  color: rgba(55, 55, 55, 1);
  box-sizing: border-box;
  padding: 0 80rpx;
  margin: 0 auto 20rpx;
}
.result .result-text-2 {
  height: 100rpx;
  display: flex;
  align-items: baseline;
  justify-content: flex-start;
  font-size: 30rpx;
  line-height: 1.5;
  color: rgba(55, 55, 55, 1);
  box-sizing: border-box;
  padding: 0 80rpx;
}

.result .result-text-1 .value {
  font-size: 40rpx;
  color: rgba(208, 63, 33, 1);
}


.result .test-items {
  height: 380rpx;
  margin: 0 auto 0;
  box-sizing: border-box;
  padding: 60rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.result .test-items .test-item {
  /* margin: 0 auto 40rpx; */
  width: 100%;
  box-sizing: border-box;
  padding: 0 80rpx;
  display: flex;
  align-items: baseline;
  justify-content: space-between;
  font-size: 30rpx;
 color: rgba(55, 55, 55, 1);
}

.result .test-items .test-item .value {
  color: rgba(208, 63, 33, 1);
}

.result .test-items .test-item.has-value .value {
   color: rgba(110, 189, 87, 1);
}
.share {
  width: 520rpx;
  color: white;
  height: 80rpx;
  font-size: 30rpx;
  background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
  border-radius: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx auto 0;
}

.button-box {
  width: 100%;
  box-sizing: border-box;
  padding: 0 60rpx;
  margin: 100rpx tuo 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.button-box .each-button {
  width: 290rpx;
  color: white;
  height: 65rpx;
  font-size: 30rpx;
  background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
  border-radius: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 40rpx auto 0;
}

.service-toast-wrapper {
  width: 750rpx;
  height: 1206rpx;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 100;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 0 200rpx;
}
.service-toast-wrapper .service-bac {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5);
}

.service-toast-wrapper .service-main {
  position: relative;
  border-radius: 20rpx;
  width: 520rpx;
  height: 678rpx;
  background: #fff url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100% auto;
  box-sizing: border-box;
  padding: 35rpx 0 40rpx;
}

.service-toast-wrapper .service-main .close-icon {
  position: absolute;
  top: 18rpx;
  right: 30rpx;
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAGFBMVEUAAAD///////////////////////////8jfp1fAAAAB3RSTlMAqgOlVPpTp3LEIgAAAGNJREFUGNNjYDFigALlBAbXQgUIm0k8hMG8XAjCUSwvBmKgFFgCKAomgAAkCCGhEhAKJgShISJQKYgEVAosAZMCS8CkIBIIDoYyhAEYRiMsRXUOikNRvIDiORRvowQIclA5AAAJyiW33lyjJwAAAABJRU5ErkJggg==') center no-repeat;
  background-size: cover; 
}

.service-toast-wrapper .service-main .service-header {
  width: 100%;
  height: 186rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 10rpx 0 35rpx;
}

.service-toast-wrapper .service-main .service-header::before {
  content: '联系客服';
  display: block;
  font-size: 36rpx;
  font-weight: bold;
  color: #fff;
}
.service-toast-wrapper .service-main .service-header::after {
  content: '';
  display: block;
  width: 273rpx;
  height: 186rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}

.service-toast-wrapper .service-main .service-text {
  width: 480rpx;
  font-size: 26rpx;
  line-height: 50rpx;
  color: #747474;
  margin: 130rpx auto 60rpx;
}

.service-toast-wrapper .service-main .service-phone {
  width: 450rpx;
  height: 65rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: linear-gradient(to right, #3d8af7, #71c5fa);
  border-radius: 32.5rpx;
  font-size: 26rpx;
  color: #fff;
  margin: 0 auto;
}




