/* pages/dailySign/dailySign.wxss */
.day-sign {
  width: 750rpx;
  height: 1206rpx;
}

.day-sign .bac {
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #c8e6fc; 
}

.day-sign .bac::before {
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 205rpx;
  background: #5b8edb;
}
.day-sign .bac::after {
   content: '';
  display: block;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 151rpx;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100% auto;
}

.day-sign .main {
  position: relative;
  z-index: 10;
}

.day-sign .main .header-area  {
  width: 100%;
  height: 145rpx;
  box-sizing: border-box;
  padding: 0 0 0 50rpx;
  display: flex;
  align-items: center;
  justify-content: space-between; 
}

.day-sign .main .header-area .header-desc {
  min-height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  font-size: 30rpx;
  color: #fff;
}

.day-sign .main .header-area .header-desc::before {
  content: '';
  display: block;
  width: 70rpx;
  height: 70rpx;
  margin: 0 25rpx 0 0;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}

.day-sign .main .header-area .sign-status {
  width: 149rpx;
  height: 55rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top-left-radius: 27.5rpx;
  border-bottom-left-radius: 27.5rpx;
  font-size: 26rpx;
  color: #3d8af7;
}

.day-sign .main .card {
  width: 647rpx;
  min-height: 927rpx;
  margin: 0 auto;
  background: #fff;
  box-shadow: 4rpx 4rpx 4rpx 0 rgba(0, 0, 0, .1);
  border-radius: 26rpx;
  box-sizing: border-box;
  padding: 50rpx 0;
}

.day-sign .main .card .card-head {
  position: relative;
  width: 100%;
  box-sizing: border-box;
  padding: 0 115rpx 0 35rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.day-sign .main .card .card-head .date-area {
  min-width: 240rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.day-sign .main .card .card-head .date-area .month {
  font-size: 80rpx;
  font-weight: bold;
  color: #000;
  line-height: 1;
}

.day-sign .main .card .card-head .date-area .main-date-box {
  margin: 0 0 0 30rpx;
}

.day-sign .main .card .card-head .date-area .main-date-box .week {
  font-size: 30rpx;
  color: #000;
  line-height: 1;
  font-weight: bold;
  margin: 0 0 10rpx;
}

.day-sign .main .card .card-head .date-area .main-date-box .year-month {
  font-size: 26rpx;
  color: #9fa0a0;
  line-height: 1;
}

.day-sign .main .card .card-head .contant-sign {
  font-size: 20rpx;
  color: #000;
  font-weight: bold;
}

.day-sign .main .card .card-head .contant-sign .sign-count{
  display: inline-block;
  color: #d03f21;
  margin: 0 0 0 8rpx;
}

.day-sign .main .card .card-head .calendar-icon {
  position: absolute;
  width: 83rpx;
  height: 75rpx;
  right: 20rpx;
  top: -5rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}

.day-sign .main .card .calendar-area {

}

.day-sign .main .card .calendar-area .weeks-box{
  display: flex;
}

.day-sign .main .card .calendar-area .weeks-box .weeks-item {
  width: 92rpx;
  height: 26rpx;
  margin: 65rpx auto 0;
  display: flex;
  align-content: center;
  justify-content: center;
  font-size: 26rpx;
  color: #000;
  /* font-weight: 500; */
  line-height: 1;
}

.day-sign .main .card .calendar-area .calendar-box {
  margin: 35rpx auto 0;
  min-height: 390rpx;
  display: flex;
  align-items: flex-start;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.day-sign .main .card .calendar-area .calendar-box .calendar-date-item {
  width: 92rpx;
  height: 75rpx;
}

.day-sign .main .card .calendar-area .calendar-box .calendar-date-item .calendar-date {
  display: flex;
  align-item: center;
  justify-content: center;
  font-size: 26rpx;
  color: #000;
  /* font-weight: 500; */
  line-height: 1;
}

.day-sign .main .card .calendar-area .calendar-box .calendar-date-item .indicate-dot {
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  background: #3d8af7;
  margin: 12rpx auto 0;
}

.day-sign .main .card .need-sign-btn {
  width: 505rpx;
  height: 65rpx;
  margin: 0 auto;
  border-radius: 32.5rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: linear-gradient(to right, #3d8af7, #69c1f9);
  font-size: 26rpx;
  color: #fff;
}

.day-sign .main .card .not-need-sign-btn {
  width: 505rpx;
  height: 65rpx;
  margin: 0 auto;
  border-radius: 32.5rpx;
  display: flex;
  justify-content: center;
  background: #d1d1d1;
  align-items: center;
  font-size: 26rpx;
  color: #fff;
}
.day-sign .main .card .sign-indicate {
  font-size: 20rpx;
  color: #747474;
  line-height: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 15rpx auto 0;
}

.day-sign .main .card .progress-box {
  margin: 50rpx auto 0;
}
.day-sign .main .card .progress-box .progress-top {
  width: 100%;
  box-sizing: border-box;
  padding: 0 15rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.day-sign .main .card .progress-box .progress-top .progress-top-item {
  width: 28rpx;
  height: 25rpx;
  font-size: 20rpx;
  line-height: 1;
  color: #000;
}
.day-sign .main .card .progress-box .progress-top .progress-top-item.gift-icon {
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}

.day-sign .main .card .progress-box .progress-main {
  position: relative;
  width: 590rpx;
  height: 2rpx;
  margin: 10rpx auto 0;
  background: #d1d1d1;
}

.day-sign .main .card .progress-box .progress-main .progress-dot {
  position: absolute;
  top: 50%;
  transform: translate3d(-50%, -6rpx, 0);
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background: #d1d1d1;
}

.day-sign .main .card .progress-box .progress-main .progress-dot.ten-day {
  left: 33%;
}
.day-sign .main .card .progress-box .progress-main .progress-dot.fifteen-day {
  left: 50%;
}
.day-sign .main .card .progress-box .progress-main .progress-dot.thirty-day {
  left: 100%;
}
.day-sign .main .card .progress-box .progress-main .progress-dot .dot-inner {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: #408ef7;
}

.day-sign .main .card .progress-box .progress-main .progress {
  position: absolute;
  left: 0;
  top: 0;
  height: 4rpx;
  transform: translate3d(0, -2rpx, 0);
  background: #408ef7;
}

.day-sign .main .card .progress-box .progress-bottom {
  position: relative;
  width: 590rpx;
  height: 2rpx;
  margin: 10rpx auto 0;
}

.day-sign .main .card .progress-box .progress-bottom .bottom-text {
  position: absolute;
  transform: translate3d(-50%, 0, 0);
  font-size: 20rpx;
  color: #000;
  line-height: 1;
  white-space: nowrap;
}

.day-sign .main .card .progress-box .progress-bottom .bottom-text.ten-day {
  left: 33%;
}
.day-sign .main .card .progress-box .progress-bottom .bottom-text.fifteen-day {
  left: 50%;
}
.day-sign .main .card .progress-box .progress-bottom .bottom-text.thirty-day {
  left: 98%;
}

.day-sign .show-sign-days {
  position: fixed;
  z-index: 20;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.day-sign .show-sign-days .toast-bac {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .2);
}

.day-sign .show-sign-days .sign-toast-card {
  position: relative;
  width: 520rpx;
  height: 678rpx;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  background-size: 100% auto;
  box-sizing: border-box;
  padding: 45rpx 0 0;
}

.day-sign .show-sign-days .sign-toast-card .sign-toast-icon {
  width: 136rpx;
  height: 123rpx;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
  margin: 0 auto 145rpx;
}

.day-sign .show-sign-days .sign-toast-card .close-icon {
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAGFBMVEUAAAD///////////////////////////8jfp1fAAAAB3RSTlMAqgOlVPpTp3LEIgAAAGNJREFUGNNjYDFigALlBAbXQgUIm0k8hMG8XAjCUSwvBmKgFFgCKAomgAAkCCGhEhAKJgShISJQKYgEVAosAZMCS8CkIBIIDoYyhAEYRiMsRXUOikNRvIDiORRvowQIclA5AAAJyiW33lyjJwAAAABJRU5ErkJggg==') center no-repeat;
  background-size: cover;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
}

.day-sign .show-sign-days .sign-toast-card .text-1{
  font-size: 46rpx;
  color: #3d8af7;
  line-height: 1;
  text-align: center;
  margin: 0 auto 50rpx;
}
.day-sign .show-sign-days .sign-toast-card .text-2{
  font-size: 26rpx;
  color: #747474;
  line-height: 1;
  text-align: center;
  margin: 0 auto 25rpx;
}

.day-sign .show-sign-days .sign-toast-card .text-3{
  font-size: 26rpx;
  color: #747474;
  line-height: 1;
  text-align: center;
  margin: 0 auto 75rpx;
}

.day-sign .show-sign-days .sign-toast-card  .toast-confirm {
  width: 450rpx;
  height: 65rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  background: linear-gradient(to right, #3d8af7, #61bef9);
  border-radius: 32.5rpx;
  font-size: 2t6rpx;
  color: #fff;
}


