/**index.wxss**/
.button {
    /* margin: 40rpx 40rpx 0rpx 40rpx; */
    width: 520rpx;
    height: 80rpx;
    background: rgb(255, 255, 255);
    border-radius: 80rpx;
    font-size: 36rpx;
    text-align: center;
    line-height: 80rpx;
    color: rgba(89, 94, 96, 1);
    position: relative;
    box-shadow: 5rpx 5rpx 5rpx 0rpx rgba(0, 0, 0, .1);
}

.main-container {
  position: relative;
  z-index: 10;
  box-sizing: border-box;
  padding: 200rpx 0 0;
}
.goTesting {
    /* margin-top: 290rpx; */
}

.goNearby {
    margin-top: 40rpx;
}

.logo {
    width: 252rpx;
    height: 324rpx;
    display: block;
    margin: 0 auto 300rpx;
}

.bg {
    width: 750rpx;
    height: 1334rpx;
    position: absolute;
}

.body{
  position: relative;
}

.can-not-sign-wrapper {
  z-index: 1000;
  position: fixed;
  width: 750rpx;
  height: 1206rpx;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}
.can-not-sign-wrapper .bac {
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .5);
  width: 100%;
  height: 100%;
}
.can-not-sign-wrapper .card {
  position: relative;
  width: 520rpx;
  height: 678rpx;
  background: url('data:image/png;base64,') 0 0 no-repeat;
  background-size: cover;
  box-sizing: border-box;
  padding: 50rpx 0 0;
}

.can-not-sign-wrapper .card .icon {
  width: 198rpx;
  height: 173rpx;
  margin: 0 auto;
  background: url('data:image/png;base64,') center no-repeat;
  background-size: cover;
}

.can-not-sign-wrapper .card .close-icon {
  width: 24rpx;
  height: 24rpx;
  position: absolute;
  right: 25rpx;
  top: 25rpx;
  background: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABgAAAAYBAMAAAASWSDLAAAAElBMVEUAAAD///////////////////8+Uq06AAAABXRSTlMAqqVU+ryx4wkAAABeSURBVBjTY2BWYIACJgMGk0AYR9SZQTVUAMJmDA0C4kCoBFAUQjCABCEkVAJKwYTANFwCzAALwKTAEjApkARcCi6B4CCUYRiAMBrDUoRzUB2K4gUUz6F4GyVAUIIKALiEGC2SY+aWAAAAAElFTkSuQmCC') center no-repeat;
  background-size: cover;
}

.can-not-sign-wrapper .card .text-1 {
  text-align: center;
  font-size: 48rpx;
  color: #3d8af7;
  line-height: 1;
  margin: 100rpx auto 0;
}

.can-not-sign-wrapper .card .text-2 {
  width: 270rpx;
  font-size: 30rpx;
  color: #747474;
  line-height: 1.5;
  white-space: normal;
  margin: 55rpx auto 0;
}
.can-not-sign-wrapper .card .btn {
  width: 450rpx;
  height: 65rpx;
  border-radius: 32.5rpx;
  background: linear-gradient(to right, #3d8af7, #61bef9);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  color: #fff;
  margin: 60rpx auto 0;
}
