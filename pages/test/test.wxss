/* pages/test/test.wxss */
.viewTest {
    position: relative;
    overflow: hidden;
}

.currentResultToastWrapper {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0, 0, .1);
  z-index: 1000;
}
.currentResultToastWrapper .currentResultToast {
  width: 600rpx;
  box-sizing: border-box;
  padding: 80rpx 60rpx 60rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 10rpx 10rpx 10rpx 10rpx rgba(0, 0, 0, .2);
}

.currentResultToastWrapper .currentResultToast .toast-header {
  font-size: 36rpx;
  color: #172137;
  text-align: center;
  line-height: 1.5;
}
.currentResultToastWrapper .currentResultToast .toast-text {
  font-size: 32rpx;
  color: #172137;
  text-align: center;
  line-height: 1.5;
  box-sizing: border-box;

}
.currentResultToastWrapper .currentResultToast .toast-event {
  width: 260rpx;
  height: 80rpx;
  margin: 30rpx auto 0;
  /* background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%); */
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: lightseagreen;
  padding: 0 40rpx;
}
.scrollviewUp {
    width: 100%;
    background-color: black;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.scrollviewUp {
    display: block;
    z-index: 2;
    height: 80rpx;
    white-space: nowrap;
}

.scrollviewUp view {
    text-align: center;
    line-height: 80rpx;
    box-sizing: border-box;
    margin-left: 30rpx;
    height: 80rpx;
    color: #d4d9dd;
    font-size: 32rpx;
    display: inline-block;
}

.scrollSecond {
    margin-top: 80rpx;
}

.scrollviewUp .currentTap {
    color: #ffffff;
    border-bottom: 6rpx solid #ffffff;
}

.scrolly {
    background-color: blue;
}

.fullScreen {
  position: relative;
  z-index: 999;
}
.presbyopia {
  box-sizing: border-box;
  padding: 40rpx 0 0;
}
.presbyopia .test-indication {
  box-sizing: border-box;
  padding: 0 40rpx;
  margin: 30rpx auto 40rpx;
  font-size: 30rpx;
  color: #172137;
  line-height: 1.3;
}
.presbyopia .presbyopia-img {
    width: 670rpx;
    display: block;
    margin: 0 auto;
}

.presbyopia .presbyopia-select-area {
  box-sizing: border-box;
  padding: 0 40rpx;
  margin: 60rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.presbyopia .presbyopia-select-area button {
  width: 300rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #fff;
  border-radius: 10rpx;
}

.presbyopia .presbyopia-select-area button:first-child {
  border:1px solid #e22d00;
  color: #e22d00;
}
.presbyopia .presbyopia-select-area button:last-child {
  border:1px solid green;
  color: green;
}

.maculafundus {

}
.maculafundus {
  box-sizing: border-box;
  padding: 40rpx 0 0;
}
.maculafundus .test-indication {
  box-sizing: border-box;
  padding: 0 40rpx;
  margin: 30rpx auto 40rpx;
  font-size: 30rpx;
  color: #172137;
  line-height: 1.3;
}
.maculafundus .maculafundus-img {
    width: 670rpx;
    height: 667rpx;
    display: block;
    margin: 0 auto;
}

.sancanvans {
    margin-top: 20rpx;
}

.san {
    display: block;
    height: 60rpx;
    width: 100%;
    line-height: 60rpx;
    text-align: center;
    font-size: 40rpx;
}

.radio-group {
    margin-top: 13px;
    text-align: center;
}

.radio {
    margin-bottom: 10rpx;
    font-size: 32rpx;
    display: block;
}

.sanbtn {
    color: white;
    margin-top: 30rpx;
    background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
    border-radius: 50px;
    width: 80%;
}

.viewTable {
    height: 60rpx;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    background-color: #f5f3e6;
    width: 100%;
}

.viewTable view {
    width: 50px;
    height: 50px;
    display: inline-block;
}

.viewTable .firstView {
    height: 48px;
    padding-left: 20px;
}

.viewTable .lastView {
    width: 48px;
    height: 48px;
}

.viewTable text {
    color: #55544f;
    display: block;
    font-size: 20rpx;
}

.ceText {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 32rpx;
    color: #ffffff;
    background-color: #374140;
}

.btns {
    margin-top: 10px;
    width: 100%;
    position: relative;
    overflow: hidden;
    height: 288rpx;
    text-align: center;
    line-height: 288rpx;
}

.ce {
    position: absolute;
    width: 120rpx;
    height: 60rpx;
    color: white;
    font-size: 14px;
    line-height: 60rpx;
    background-image: linear-gradient(-144deg,#399DF7 0%,#136BFE 100%);
}

.up {
    left: 50%;
    margin-left: -60rpx;
}

.down {
    top: 60%;
    left: 50%;
    margin-left: -60rpx;
}

.center {
    left: 50%;
    top: 32%;
    margin-left: -40px;
    width: 80px;
    background: #ababab;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 14px;
}

.left {
    left: 20%;
    top: 30%;
}

.right {
    right: 20%;
    top: 30%;
}

.cecanvas {
    position: relative;
}

.floatCanvas {
    position: absolute;
    top: 0;
    background-color: red;
}

.textColor text {
    color: #f90042;
}

.table {
    height: 100%;
    width: 100%;
}

.table canvas {
    display: block;
}

.seDo {
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #374140;
    overflow: hidden;
    padding-left: 30rpx;
    padding-right: 30rpx;
    position: relative;
}

.seDo view {
    display: inline-block;
}

.doCircle {
    margin-top: 30rpx;
    width: 30rpx;
    height: 30rpx;
    border-radius: 16rpx;
    border: 1rpx solid #ea614f;
    background-color: white;
    position: relative;
}

.seTF {
    position: absolute;
    font-size: 16px;
    color: #e22d00;
    top: -95%;
    left: 15%;
}

.doLine {
    margin-left: 1rpx;
    margin-right: 1rpx;
    width: 125rpx;
    height: 15rpx;
    border-top: 1rpx solid #d4d9dd;
}

.hasLight {
    margin-top: 30rpx;
    width: 30rpx;
    height: 30rpx;
    border-radius: 16rpx;
    border: 1rpx solid #ea614f;
    position: relative;
    background-color: #ea614f;
}

.imageView {
    margin-top: 20rpx;
    position: relative;
    width: 100%;
    height: 40%;
    overflow: hidden;
}

.imageView image {
    width: 60%;
    height: 100%;
    position: absolute;
    background-size: contain;
}

.subLeft {
    left: -50%;
}

.showImage {
    left: 50%;
    margin-left: -30%;
}

.subRight {
    right: -50%;
}

.fillIn {
    padding-top: 5%;
    position: relative;
    width: 100%;
    height: 30%;
    text-align: center;
}

.fillIn text {
    color: #1b1b1b;
    font-size: 16px;
}

.bottomUl {
    z-index: 2;
    top: 25%;
    position: absolute;
    left: 50%;
    margin-left: -10%;
    width: 20%;
    height: 65%;
    overflow: hidden;
}

.bottomUlplus {
    z-index: 2;
    top: 25%;
    position: absolute;
    left: 50%;
    margin-left: -10%;
    width: 20%;
    height: 50%;
    overflow: hidden;
}

.transitionX {
    margin-top: 20px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.mask {
    z-index: 1;
    width: 20%;
    top: 50%;
    position: absolute;
    left: 50%;
    margin-left: -10%;
    height: 26px;
    border-top: 2px solid #66c4e6;
    border-bottom: 2px solid #66c4e6;
}

.bottomLi {
    text-align: center;
    height: 30px;
    width: 100%;
    color: #1b1b1b;
    font-size: 20px;
}

.submitResult {
    width: 700rpx;
    margin: 0 auto;
    height: 8%;
    z-index: 2;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.submitResult button {
    color: white;
    line-height: 250%;
    text-align: center;
    display: inline-block;
    width: 330rpx;
    background: #606060;
    border-radius: 22rpx;
}

.submitResult .surebtn {
    background-image: linear-gradient(-144deg,#39CFF7 0%,#136BFE 100%);
    border-radius: 22rpx;
}

.centerTwoF {
    position: absolute;
    left: 25%;
    width: 20%;
    height: 65%;
    top: 30%;
}

.centerTwoS {
    position: absolute;
    right: 25%;
    width: 20%;
    height: 65%;
    top: 30%;
}

.maskTwo {
    z-index: 1;
    width: 100%;
    top: 39%;
    height: 26px;
    position: absolute;
    border-top: 2px solid #66c4e6;
    border-bottom: 2px solid #66c4e6;
}

.bottomUlTwo {
    z-index: 2;
    top: 0%;
    position: absolute;
    width: 100%;
    height: 100%;
    overflow: hidden;
}